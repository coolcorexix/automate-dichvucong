{
  "manifest_version": 3,
  "name": "DichVuCong Assistant",
  "version": "1.0.0",
  "description": "A Chrome extension to assist with DichVuCong website",
  "permissions": ["activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  }

}
